package leetcode;

public class LargestNumber179 {
	public String largestNumber(int[] nums) {

		return "";
	}

	public int compare(int num1, int num2) {
		if (num1 == num2) {
			return 0;
		}
		int i = 1;
		int num1Len = 0;
		int num2Len = 0;
		while (num1 / (int)Math.pow(10, i) != 0) {
			i++;
		}
		num1Len = i;
		i = 1;
		while (num2 / (int)Math.pow(10, i) != 0) {
			i++;
		}
		num2Len = i;
		System.out.println("1:"+num1Len+" 2:"+num2Len);
		if ((num1 + num2 * Math.pow(10, num1Len - 1)) < (num2 + num1 * Math.pow(10, num2Len - 1))) {
			return -1;
		} else {
			return 1;
		}
	}

	public static void main(String[] args) {

		System.out.println(new LargestNumber179().compare(3, 34));
	}
}
